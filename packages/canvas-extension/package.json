{
  "name": "@cycle/canvas-extension",
  "version": "0.1.1",
  "description": "Cycle canvas extension",
  "author": "Yarden Livnat",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepack": "npm run clean && npm run build",
    "watch": "tsc -w",
    "link": "jupyter labextension link ."
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@jupyterlab/application": "^0.11.1",
    "@jupyterlab/coreutils": "^0.11.1",
    "@jupyterlab/docregistry": "^0.11.1",
    "@jupyterlab/notebook": "^0.11.1",
    "@jupyterlab/services": "^0.50.2",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/disposable": "^1.1.2",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/widgets": "^1.5.0",
    "@cycle/canvas": "^0.1.0"
  },
  "devDependencies": {
    "rimraf": "^2.5.2",
    "typescript": "^2.7.0-dev.20171027"
  }
}
